<%= simple_form_for @contact, url: admin_contacts_path(vendor_id: params[:vendor_id]) , method: :post, remote: true do |f| %>
  
    <div id="form_message" class="two fields red" style="margin-left: 15px;display:none;" ></div>

    <%= render 'form', f: f, wide: :two %>
    <%= f.submit 'Submit' %>

<% end %>